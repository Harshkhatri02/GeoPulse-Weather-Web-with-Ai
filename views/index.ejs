/*Nav----------------------------*/
nav{
    display: flex;
    align-items: center;
    justify-content: space-around;
}


nav .logo h4{
    font-size: 2rem;
    background-image: url('cloud.webp');
    background-repeat: no-repeat;
    background-clip: text;
    color: transparent;
    text-shadow:-1px -2px white;
  }
  
 
  nav button{
    cursor: pointer;
    background-color: var(--body-color-);
    display: flex;
    justify-content: center;
    align-items: center;
    gap: 0.3rem;
  }
.logo{
  border:white 1px solid;
  padding: 0.2rem 1rem 0.2rem 1rem;
  border-radius: 2rem;
  background-color: var(--body-color-);
  display: flex;
  justify-content: center;
  align-items: center;
  gap: 0.5rem;
  animation: roll-in-left .5s forwards ease;
}

@keyframes roll-in-left {
  0% {
    transform: translateX(-800px) rotate(-540deg);
    opacity: 0;
  }
  100% {
    transform: translateX(0) rotate(0deg);
    opacity: 1;
  }
}

  .logo img{
    height:3rem;
    width: 3rem;
    object-fit: contain;
  }
  nav ul{
    font-size: 1.3rem;
    display: flex;
    align-items: center;
    justify-content: center;
    gap: 1rem;
}
nav ul,button{
  color: white;
    padding:0.7rem;
    border-radius: 1.7rem;
    background-color:var(--body-color-);
    border: rgb(255, 255, 255) solid 0.5px ;
}
nav ul li{
  text-decoration: none;
  list-style: none;
  position: relative;
    padding: 0.2rem;
    cursor: pointer;
  }
nav ul li::after {
  content: '';
  height: 0%;
  border-radius: 1rem;
  position: absolute;
  bottom: 0;
  display: block;
  justify-content: center;
  border: rgb(255, 255, 255) solid 1px;
  box-shadow: rgba(17, 17, 26, 0.1) 0px 4px 16px, rgba(17, 17, 26, 0.1) 0px 8px 24px, rgba(17, 17, 26, 0.1) 0px 16px 56px;
  transition: width 0.5s ease;
}

/* Hover state animation */
nav ul li:hover::after {
  animation: underlineAfter 0.5s forwards ease-in-out;
}

/* Non-hover state animation */
nav ul li:not(:hover)::after {
  animation: reverseUnderlineAfter 0.5s forwards ease-in-out;
}

/* Keyframes with only what's changing */
@keyframes underlineAfter {
  0% {
    width: 0%;
  }
  100% {
    width: 90%;
  }
}

@keyframes reverseUnderlineAfter {
  0% {
    width: 90%;
  }
  100% {
    width: 0%;
    display: none;
  }
}
  
