    .search-bar {
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    column-gap: 1rem;
    padding: 20px;
    position: relative;
}

.search-bar input {
    z-index: 1;
    background-color: white;
    width: 60%;
    height: 40px;
    border-radius: 20px;
    padding: 10px 20px;
    outline: none;
}

#searchSuggestions {
    position: absolute;
    z-index: 1;
    background-color: white;
    border-bottom-left-radius: 1rem;
    border-bottom-right-radius: 1rem;
    box-shadow: 0px 8px 16px rgba(0,0,0,0.1);
    height: auto;
    top: calc(10px + 40px); /* Position below input (container padding + input height) */
    left: 45.7%;
    transform: translateX(-50%);
    width: 57%; /* Match the input width */
    margin-top: 5px; /* Small gap between input and suggestions */
}
    .styles{
      padding: 5px;
      padding-left: 5px;
      cursor: pointer;
    }
    .search-bar input::placeholder {
      color: #999;
    }
    .search-bar input:focus{
      border: rgb(30, 30, 30) solid 1.5px;
    }
    .search-bar button {
      background-color: #281f3b;
      width: 40px;
      height: 40px;
      border: none;
      border-radius: 50%;
      outline: none;
      display: flex;
      justify-content: center;
      align-items: center;
    }
    .search-bar input,.search-bar button{
      border: grey 0.8px solid;
    }
    .now {
      display: flex;
      align-items: center;
      justify-content: space-between;
      padding: 20px;
    }
    .geolocation-btn{
      padding: 0.5rem;
    }
    .now h2 {
      font-size: 24px;
      font-weight: normal;
    }
    
    .now p {
      font-size: 18px;
      font-weight: normal;
    }

    .forecast {
      padding: 20px;
      display: grid;
      align-items: center;
      justify-content: space-around;
      grid-template-columns: 30%  60%;
      flex-wrap: wrap;
    }
    
    .forecast h2 {
      font-size: 24px;
      font-weight: normal;
    }
    
    .forecast-days {
      display: flex;
      align-items: center;
      justify-content: space-between;
      width: 80%;
    }
    
    .forecast-days .day {
      display: flex;
      flex-direction: column;
      align-items: center;
    }

    .forecast-days .day p {
      font-size: 18px;
      font-weight: normal;
    }

    .todays-highlights {
      display: grid;
      grid-template-columns: repeat(4,1fr);
      overflow-x: auto;
      scrollbar-width: none;
      grid-gap: 20px;
      padding: 20px;
    }
    
    .todays-highlights h2 {
      font-size: 24px;
      font-weight: normal;
      grid-column: span 4;
    }
    
    .todays-highlights .highlight {
      
      border-radius: 20px;
      padding: 20px;
    }

    .todays-highlights .highlight p {
      font-size: 18px;
      font-weight: normal;
    }

    .location {
      text-align: center;
      padding: 20px;
    }

    .location p {
      font-size: 18px;
      font-weight: normal;
    }

    .time {
      display: flex;
      align-items: center;
      justify-content: space-between;
      padding: 20px;
    }
    
    .time .time-data {
      display: flex;
      flex-direction: column;
      align-items: center;
    }
    
    .time .time-data p {
      font-size: 18px;
      font-weight: normal;
    }
    
  
/*Temp*/

