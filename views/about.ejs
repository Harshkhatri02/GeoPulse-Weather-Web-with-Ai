#overflow{
    position: absolute;
    height: 100%;
    width: 100%;
    white-space:nowrap;
}
.inner{
    height: 100%;
    width:100%;
    display:flex;
    overflow: auto;
    scrollbar-width:0; 
    -ms-overflow-style: none;  
    transition: scroll 1s ease-in;
}
.inner::-webkit-scrollbar {
    display: none; 
}
.inner > div {
    flex: 0 0 100%;
    box-sizing: border-box; 
}
.inner > div:not(:last-child) {
    margin-right: 10px;
}
.inner>div p {
    max-width: 100%; 
    overflow-y: hidden;
    text-overflow: ellipsis;
    white-space: wrap; 
